<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pc.gateway.mapper.SysAnonRouteMapper">
    <select id="getAnonRouteById" parameterType="java.lang.String" resultType="com.pc.gateway.model.route.SysAnonRoute">
        SELECT
        sysid   sysId,
        syspath sysPath,
        method,
        status,
        remark
        FROM test.s_sys_anonymous_rout
        WHERE sysid = #{sysId}
    </select>

    <select id="getAllAnonRoute" resultType="com.pc.gateway.model.route.SysAnonRoute">
        SELECT
        sysid   sysId,
        syspath sysPath,
        method,
        status,
        remark
        FROM test.s_sys_anonymous_rout
    </select>
</mapper>